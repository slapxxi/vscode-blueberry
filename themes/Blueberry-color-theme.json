  "tokenColors": [
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.from-file",
        "meta.diff.header.git",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#a2bffc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Text",
      "scope": [
        "variable.parameter",
        "entity.name.function.tagged-template variable.other.property",
        "entity.other.attribute-name",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#575e7f"
        // "foreground": "#_5a5c7a"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "variable.object.property",
        "support.type.property-name",
        "meta.tag.attributes entity.other.attribute-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#40455e",
        // "foreground": "#_444662",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Text Cursive",
      "scope": ["meta.indexer.declaration variable.parameter"],
      "settings": {
        "foreground": "#575e7f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#40455e"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "support.function.misc",
        "support.function.gradient",
        "support.function.name.postcss",
        "support.function.transform.css",
        "support.function.calc.css",
        "support.function.transform.scss",
        "support.function.calc.scss",
        "entity.name.tag support.class.component",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#0f5fff"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "entity.name.tag.css",
        "entity.name.tag.scss",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element",
        "keyword",
        "punctuation.accessor",
        "storage.modifier",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#0f5fff"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.type",
        "support.constant.color",
        "support.class.console",
        // treat as a constant
        "keyword.other.unit",
        "support.type.property-name.json",
        "support.variable.object.node",
        "variable.language.this",
        "string.regexp keyword.control",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#52c5ff"
        // "foreground": "#_50c7ff"
      }
    },
    {
      "name": "Entities",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#52c5ff"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression",
        "meta.object-literal.key meta.array.literal meta.brace.square",
        "meta.object-literal.key meta.array.literal variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#52c5ff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#34384b"
        // "foreground": "#343652"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#0f5fff"
      }
    },
    {
      "name": "Destructuring",
      "scope": [
        "meta.block variable.other.readwrite.alias",
        "meta.object-binding-pattern-variable",
        "meta.parameter.object-binding-pattern variable.parameter"
      ],
      "settings": {
        "foreground": "#f0f5ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "meta.tag punctuation.definition.tag",
        "punctuation.definition",
        "punctuation.section.property-list",
        "punctuation.section.function",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#2f3242"
      }
    },
    {
      "name": "String Punctuation",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#f0f5ff"
      }
    },
    {
      "name": "Object",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#f0f5ff"
      }
    },
    {
      "name": "Name",
      "scope": [
        "entity.name.function.tagged-template",
        "entity.name.section.group-title",
        "entity.name.section.markdown",
        "entity.name.type.alias",
        "entity.name.type.class",
        "entity.name.type.enum",
        "entity.name.type.interface",
        "entity.name.type.module",
        "meta.definition.function entity.name.function",
        "meta.definition.method entity.name.function",
        "meta.definition.property entity.name.function",
        "support.constant.property-value",
        "variable.argument.css",
        "variable.argument.scss",
        "variable.scss",
        "variable.parameter.postcss",
        "new.expr entity.name.type",
        "entity.other.inherited-class",
        "variable.postcss"
      ],
      "settings": {
        "foreground": "#f0f5ff",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.scope.subject.git-commit",
        "string.template",
        "punctuation.definition.string.template",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#f0f5ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Regular Expession",
      "scope": [
        "string.regexp",
        "string.regexp keyword.operator",
        "string.regexp punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#f5cc23"
      }
    },
    {
      "name": "Markdown",
      "scope": ["beginning.punctuation.definition.list.markdown"],
      "settings": {
        "foreground": "#0f5fff"
      }
    },
    {
      "name": "Markdown Text",
      "scope": ["text.html.markdown"],
      "settings": {
        "foreground": "#f0f5ff"
      }
    },
    {
      "name": "Markup",
      "scope": ["markup.changed.git-commit", "markup.inserted.git-commit"],
      "settings": {
        "foreground": "#f5cc23"
      }
    },
    {
      "name": "Dangerous",
      "scope": ["markup.deleted.git-commit"],
      "settings": {
        "foreground": "#db3428"
      }
    }
  ]
}
