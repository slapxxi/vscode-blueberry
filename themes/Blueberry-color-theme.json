  "tokenColors": [
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#a2bffc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Text",
      "scope": [
        "variable.parameter",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#555770"
      }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "punctuation.accessor",
        "storage.type.function",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.type",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#50c7ff"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#50c7ff"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression",
        "meta.object-literal.key meta.array.literal meta.brace.square",
        "meta.object-literal.key meta.array.literal variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#50c7ff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#313141"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Destructuring",
      "scope": [
        "meta.block variable.other.readwrite.alias",
        "meta.parameter.object-binding-pattern variable.parameter",
        "meta.object-binding-pattern-variable"
      ],
      "settings": {
        "foreground": "#f3f3ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace.square",
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#37384c"
      }
    },
    {
      "name": "String Punctuation",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#f3f3ff"
      }
    },
    {
      "name": "Object",
      "scope": ["meta.object-literal.key", "variable.other.object"],
      "settings": {
        "foreground": "#f3f3ff"
      }
    },
    {
      "name": "Name",
      "scope": [
        "meta.definition.function entity.name.function",
        "meta.definition.property entity.name.function",
        "meta.definition.method entity.name.function",
        "entity.name.type.alias",
        "entity.name.type.module",
        "entity.name.type.interface",
        "entity.name.type.enum",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#f3f3ff"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted.double"],
      "settings": {
        "foreground": "#f3f3ff",
        "fontStyle": "italic"
      }
    }
  ]
}
