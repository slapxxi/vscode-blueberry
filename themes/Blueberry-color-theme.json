  "tokenColors": [
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.from-file",
        "meta.diff.header.git",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#a2bffc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Text",
      "scope": [
        "meta.paragraph.markdown",
        "variable.parameter",
        "entity.name.function.tagged-template variable.other.property",
        "entity.other.attribute-name",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#646d90"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "variable.object.property",
        "support.type.property-name",
        "meta.tag.attributes entity.other.attribute-name",
        "meta.object-literal.key",
        "variable.graphql",
        "support.class.console",
        "meta.type_params.rust"
      ],
      "settings": {
        "foreground": "#444965"
      }
    },
    {
      "name": "Meta",
      "scope": ["meta.attribute.rust"],
      "settings": {
        "foreground": "#2f3142"
      }
    },
    {
      "name": "Text Cursive",
      "scope": ["meta.indexer.declaration variable.parameter", "markup.bold.markdown"],
      "settings": {
        "foreground": "#646d90"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#444965"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "support.function.misc",
        "support.function.gradient",
        "support.function.name.postcss",
        "support.function.transform.css",
        "support.function.calc.css",
        "support.function.transform.scss",
        "support.function.calc.scss",
        "entity.name.tag support.class.component",
        "entity.name.function.tagged-template",
        "meta.function-call entity.name.function",
        "meta.function-call",
        "support.constant.math"
      ],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "entity.name.function.rust",
        "support.function.std",
        "entity.name.tag.css",
        "entity.name.tag.scss",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element",
        "keyword",
        "punctuation.accessor",
        "punctuation.symbol.match-branch",
        "storage.modifier",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "support.constant.core.rust",
        "constant",
        "support.type",
        "support.constant.color",
        "string.other.link",
        // treat as a constant
        "keyword.other.unit",
        "support.variable.object.node",
        "support.variable.object.process",
        "support.function.builtin.shell",
        "variable.language.this",
        "variable.language.self",
        "string.regexp keyword.control",
        "variable.other.constant",
        "variable.other.normal.shell",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#1ab2ff"
      }
    },
    {
      "name": "JSON Property",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#f5cc23"
      }
    },
    {
      "name": "Entities",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#646d90"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression",
        "meta.object-literal.key meta.array.literal meta.brace.square",
        "meta.object-literal.key meta.array.literal variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#1ab2ff"
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "foreground": "#2a2d3c"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Destructuring",
      "scope": [
        "meta.block variable.other.readwrite.alias",
        "meta.object-binding-pattern-variable",
        "meta.parameter.object-binding-pattern variable.parameter"
      ],
      "settings": {
        "foreground": "#f5f7ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.symbol",
        "meta.brace.round",
        "meta.brace.square",
        "meta.tag punctuation.definition.tag",
        "meta.object-binding-pattern-variable punctuation.destructuring",
        "punctuation.definition",
        "punctuation.section.property-list",
        "punctuation.section.function",
        "punctuation.separator",
        "punctuation.terminator",
        // GLSL
        "punctuation.section",
        // GraphQL
        "punctuation.graphql",
        "punctuation.operation.graphql",
        "punctuation.colon.graphql",
        "meta.brace.curly.graphql"
      ],
      "settings": {
        "foreground": "#2f3243"
      }
    },
    {
      "name": "String Punctuation",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#f5f7ff"
      }
    },
    {
      "name": "Object",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#f5f7ff"
      }
    },
    {
      "name": "Name",
      "scope": [
        "meta.function entity.name.function.rust",
        "support.other.macro",
        "entity.name.module",
        "entity.name.structure",
        "entity.name.function.definition",
        "support.module.builtin.rust",
        "entity.name.function.macro.rust",
        "meta.implementation.rust",
        "storage.class.std.rust",
        "entity.name.function",
        // "entity.name.function.tagged-template",
        "entity.name.section.group-title",
        "entity.name.section.markdown",
        "entity.name.type.alias",
        "entity.name.type.class",
        "entity.name.type.enum",
        "entity.name.type.trait",
        "entity.name.type.structure",
        "entity.name.type.enumeration",
        "entity.name.type.implementation",
        "entity.name.type.interface",
        "entity.name.type.module",
        "meta.definition.function entity.name.function",
        "meta.definition.method entity.name.function",
        "meta.definition.property entity.name.function",
        "meta.definition.variable variable.other.readwrite",
        "meta.definition.variable entity.name.function",
        "meta.definition.variable variable.other.constant",
        "meta.import variable.other.readwrite",
        "support.constant.property-value",
        "support.constant.property",
        "support.class",
        "variable.argument.css",
        "variable.argument.scss",
        "variable.scss",
        "variable.parameter.postcss",
        "new.expr entity.name.type",
        "entity.other.inherited-class",
        "variable.postcss"
      ],
      "settings": {
        "foreground": "#f5f7ff",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.scope.subject.git-commit",
        "string.template",
        "punctuation.definition.string.template",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#f5f7ff"
      }
    },
    {
      "name": "Regular Expession",
      "scope": [
        "keyword.other.debugger",
        "string.regexp",
        "string.regexp keyword.operator",
        "string.regexp punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#f5cc23"
      }
    },
    {
      "name": "Markdown",
      "scope": ["beginning.punctuation.definition.list.markdown"],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Markdown Text",
      "scope": ["text.html.markdown"],
      "settings": {
        "foreground": "#f5f7ff"
      }
    },
    {
      "name": "Markup",
      "scope": ["markup.changed.git-commit", "markup.inserted.git-commit"],
      "settings": {
        "foreground": "#f5cc23"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "comment meta.paragraph.markdown", "comment markup.bold.markdown"],
      "settings": {
        "foreground": "#2f3142"
      }
    },
    {
      "name": "Dangerous",
      "scope": ["markup.deleted.git-commit"],
      "settings": {
        "foreground": "#db3428"
      }
    }
  ]
}
