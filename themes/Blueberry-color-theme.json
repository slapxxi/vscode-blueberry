  "tokenColors": [
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#a2bffc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#1166ff"
      }
    },
    {
      "name": "Punctuation Definition",
      "scope": ["punctuation.definition"],
      "settings": {
        "foreground": "#37384c"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted.double"],
      "settings": {
        "foreground": "#f3f3ff",
        "fontStyle": "italic"
      }
    }
  ]
